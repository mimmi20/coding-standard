  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "doctrine/coding-standard": "^9.0.0",
    "friendsofphp/php-cs-fixer": "^3.8.0",
    "slevomat/coding-standard": "^7.2.1 || ^8.0.0",
    "squizlabs/php_codesniffer": "^3.7.1"
